<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ProcessSubject</servlet-name>
        <servlet-class>lecturerServlet.ProcessSubject</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessProfile</servlet-name>
        <servlet-class>lecturerServlet.ProcessProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterAndLogin</servlet-name>
        <servlet-class>lecturerServlet.RegisterAndLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessQuiz</servlet-name>
        <servlet-class>lecturerServlet.ProcessQuiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessPlayedQuiz</servlet-name>
        <servlet-class>studentServlet.ProcessPlayedQuiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessStudent</servlet-name>
        <servlet-class>studentServlet.ProcessStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProcessSubject</servlet-name>
        <url-pattern>/ProcessSubject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessProfile</servlet-name>
        <url-pattern>/ProcessProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterAndLogin</servlet-name>
        <url-pattern>/RegisterAndLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessQuiz</servlet-name>
        <url-pattern>/ProcessQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessPlayedQuiz</servlet-name>
        <url-pattern>/ProcessPlayedQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessStudent</servlet-name>
        <url-pattern>/ProcessStudent</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
